<%= form_with model: [company, employee] do |form| %>
  <div>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :email %><br>
    <%= form.email_field :email %>
  </div>

  <div>
    <%= form.label :contact %><br>
    <%= form.telephone_field :contact %>
    <% end %>
    <% end %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>