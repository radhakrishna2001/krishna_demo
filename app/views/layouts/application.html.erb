<!DOCTYPE html>
<html>
  <head>
    <title>Crmproject</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <%= javascript_importmap_tags %>
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css" rel="stylesheet">
  </head>

  <body>
    <nav class="navbar navbar-expand-lg navbar-light bg-success">
     <% if current_user %>
    <a class="navbar-brand text-light" href="#">CrmProject</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
    </button>

    <div class="collapse navbar-collapse" id="navbarNav">
      <ul class="navbar-nav">
      <li class="nav-item active">
        <a class="nav-link text-light" href="#">Home <span class="sr-only"></span></a>
      </li>

      <li><%= link_to "Company",companies_path,class:"nav-link text-light" %></li>

      <li class="nav-item">
        <a class="nav-link text-light" href="#">Product</a>
      </li>
      <li class="nav-item">
        <a class="nav-link  text-light " href="#">About</a>
      </li>
    </ul>
   </div>  
    
  
    <ul class="navbar-nav">
      <% if user_signed_in? %>
    <li><%= link_to "Logout",destroy_user_session_path,data:{turbo_method: :delete},class:"nav-link text-light"%></li>
     <% else %>
    <li><%= link_to "Signup",new_user_registration_path,class:"nav-link text-light" %></li>
    <% end %>
    </ul>
    <% end %>
    </nav>

    <!-- <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p> -->


     <%= yield %>

  </body>
</html>




